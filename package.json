{
  "name": "@oneluiz/dual-datepicker",
  "version": "4.0.0",
  "description": "A customizable dual-calendar date range picker for Angular 17+ with Reactive Forms, Signals, Headless Architecture, Plugin-Driven Presets, Multi-Range support, Time Picker, and Timezone-Safe Date Adapter",
  "keywords": [
    "angular",
    "datepicker",
    "date-range",
    "calendar",
    "dual-calendar",
    "date-picker",
    "angular-component",
    "standalone",
    "typescript",
    "reactive-forms",
    "signals",
    "control-value-accessor",
    "timezone-safe",
    "date-adapter"
  ],
  "author": "Luis Cortes",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/oneluiz/ng-dual-datepicker.git"
  },
  "bugs": {
    "url": "https://github.com/oneluiz/ng-dual-datepicker/issues"
  },
  "homepage": "https://oneluiz.github.io/ng-dual-datepicker/",
  "files": [
    "fesm2022/",
    "esm2022/",
    "*.d.ts",
    "*.metadata.json",
    "themes/",
    "README.md",
    "LICENSE",
    "THEMING.md"
  ],
  "peerDependencies": {
    "@angular/common": "^17.0.0 || ^18.0.0 || ^19.0.0 || ^20.0.0",
    "@angular/core": "^17.0.0 || ^18.0.0 || ^19.0.0 || ^20.0.0",
    "@angular/forms": "^17.0.0 || ^18.0.0 || ^19.0.0 || ^20.0.0"
  },
  "scripts": {
    "build": "ng-packagr -p ng-package.json && npm run postbuild",
    "postbuild": "mkdir -p dist/themes && cp -r dist/src/themes/* dist/themes/",
    "publish:pkg": "cd dist && npm publish --access public",
    "pack:check": "npm pack --dry-run && echo '\n\n=== Files that would be published ===' && npm pack 2>/dev/null | tail -1 | xargs tar -tzf 2>/dev/null | head -50",
    "pack:verify": "npm pack && tar -tzf *.tgz | grep -E '\\.(js|d\\.ts|metadata\\.json)$' | head -30 && rm -f *.tgz",
    "demo:serve": "ng serve demo",
    "demo:build": "ng build demo --base-href=/ng-dual-datepicker/ && cd docs && mv browser/* . 2>/dev/null || true && rm -rf browser 2>/dev/null || true && touch .nojekyll && cd ..",
    "test": "npm run test:compile && npm run test:run",
    "test:compile": "tsc -p tsconfig.test.json",
    "test:run": "node --test $(find dist-test -name '*.spec.js')",
    "test:watch": "node --test --watch dist-test/**/*.spec.js"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^18.2.14",
    "@angular/cli": "^18.2.14",
    "@angular/common": "^18.2.14",
    "@angular/compiler": "^18.2.14",
    "@angular/compiler-cli": "^18.2.14",
    "@angular/core": "^18.2.14",
    "@angular/forms": "^18.2.14",
    "@angular/platform-browser": "^18.2.14",
    "@angular/platform-browser-dynamic": "^18.2.14",
    "ng-packagr": "^18.2.1",
    "rxjs": "^7.8.2",
    "typescript": "5.5",
    "zone.js": "^0.14.10"
  },
  "packageManager": "pnpm@10.13.1+sha512.37ebf1a5c7a30d5fabe0c5df44ee8da4c965ca0c5af3dbab28c3a1681b70a256218d05c81c9c0dcf767ef6b8551eb5b960042b9ed4300c59242336377e01cfad"
}
